figure{margin:0;padding-bottom:1em}figure.limit-width img{width:40em}figure.limit-width-200 img{width:200px}figure>a{max-width:100%}figcaption{padding-left:.3rem;margin-top:.3rem;color:#666}figcaption p{font-style:italic;font-size:1em;margin:0}figcaption h4{margin:0;font-style:normal}.image-border img,.image-border a img{margin:0;padding:4px;-webkit-box-shadow:rgba(34,25,25,.4) 0 1px 3px;box-shadow:rgba(34,25,25,.4) 0 1px 3px;background-color:#fff}#lightbox-loading{display:flex;justify-content:center;align-items:center;align-self:center;position:relative;background:#fff;box-shadow:0 0 15px 5px rgba(0,0,0,.5);opacity:1;border-radius:100%;width:90px;height:90px;cursor:pointer;transition:all 300ms ease-in-out}#lightbox-loading.close{animation:overlayClose 300ms ease-in-out forwards}.spinner{height:60px;width:60px;position:relative;display:flex;align-items:center;justify-content:center}.spinner::before{content:"";display:block;height:100%;width:100%;animation:rotation 1s linear infinite;border:3px solid #0c5aa6;border-top-color:transparent;border-radius:100%;background:transparent;border-radius:100%}#lightbox-container{position:fixed;top:0;left:0;right:0;bottom:0;display:flex;height:100%;opacity:0;visibility:hidden}#lightbox-container.open{opacity:1;visibility:visible}#lightbox-container.close{visibility:hidden;opacity:0;animation:lightboxClose 500ms ease-in-out forwards}#lightbox-container #lightbox{position:fixed;width:100%;height:100%;display:flex;justify-content:center;align-items:center;overflow:hidden;padding:0;margin:0;opacity:0;visibility:hidden}#lightbox-container #lightbox.open{transform-origin:50% -10% scale(0);visibility:visible;opacity:1;animation:lightboxOpen 500ms ease-in-out forwards}#lightbox-container #lightbox.close{transform-origin:50% 50% scale(1);visibility:hidden;opacity:0;animation:lightboxClose 500ms ease-in-out forwards}#lightbox-container #lightbox img{max-width:100vw;max-height:100vh;cursor:pointer;box-shadow:0 4px 15px 5px rgba(0,0,0,.6)}@media(min-width:800px) and (min-height:800px){#lightbox img{margin:15px;max-width:calc(100vw - 50px);max-height:calc(100vh - 30px)}}@keyframes lightboxOpen{0%{transform:translateY(-100%) scale(0.001);visibility:visible;opacity:0}80%{opacity:1}95%{transform:scale(1.001)}100%{transform:translateY(0%) scale(1)}}@keyframes lightboxClose{0%{visibility:visible;opacity:1;transform:translateY(0%) scale(1)}20%{transform:translateY(-1%) scale(1.001)}80%{opacity:.7}99%{transform:translateY(100%) scale(0.0001)}100%{opacity:0;visibility:hidden}}@keyframes rotation{from{transform:rotate(0deg)}to{transform:rotate(359deg)}}img{max-width:100%;height:auto}.contact-info ul{list-style:none}.contact-info li{display:flex}.contact-info li:before{content:'';display:none}.contact-info a div{display:flex;align-items:center}.contact-info .fa{min-width:1.2em}